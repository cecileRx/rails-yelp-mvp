<div class="container col-6 ">
  <h2>Add your restaurant</h2>
<%= simple_form_for [@restaurant] do |f| %>
  <%= f.input :name %>
  <%= f.input :category %>
  <%= f.input :phone_number %>
  <%= f.input :address %>
  <%= f.submit 'add a restaurant', class: 'btn btn-primary w-100'%>
  <% end %>
</div>

